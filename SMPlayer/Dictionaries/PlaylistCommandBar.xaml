                                x:Name="HighContrastBorder"
                                Grid.ColumnSpan="2"
                                VerticalAlignment="Stretch"
                                x:DeferLoadStrategy="Lazy"
                                Stroke="{ThemeResource CommandBarHighContrastBorder}"
                                StrokeThickness="1"
                                Visibility="Collapsed" />

