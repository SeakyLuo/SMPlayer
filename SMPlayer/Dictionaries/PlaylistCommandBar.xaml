                            Height="{TemplateBinding Height}"
                            MinHeight="{ThemeResource AppBarThemeCompactHeight}"
                            Margin="{TemplateBinding Padding}"
                            VerticalAlignment="Top"
                            Background="{TemplateBinding Background}"
                            XYFocusKeyboardNavigation="Enabled">

